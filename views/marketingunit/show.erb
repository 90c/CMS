<!doctype html>
<%#include "marketingunit.h" %>
<% tfetch(Marketingunit, marketingunit); %>
<html>
<head>
	<meta http-equiv="content-type" content="text/html;charset=UTF-8" />
  <title><%= controller()->name() + ": " + controller()->activeAction() %></title>
	<link rel="stylesheet" type="text/css" href="/easyui/themes/default/easyui.css">
	<link rel="stylesheet" type="text/css" href="/easyui/themes/icon.css">
	<link rel="stylesheet" type="text/css" href="common.css">
	<script type="text/javascript" src="/easyui/jquery.min.js"></script>
	<script type="text/javascript" src="/easyui/jquery.easyui.min.js"></script>
	<style type="text/css">
#customers
  {
  width:510px;
  border-collapse:collapse;
  empty-cells: hide;
  position: relative;
  left: 0px;
  top: 0px;
  }

#customers td, #customers th 
  {
  font-size:0.7em;
  border:1px solid #95B8E7;
  padding:6px 7px 5px 7px;
  }

#customers th 
  {
  font-size:0.7em;
  text-align:center;
  padding-top:5px;
  padding-bottom:4px;
  background-color:#F3F3F3;
  color:black;
  }

#customers tr, #customers td
  {
  color:#000000;
  }
</style>
</head>
 <body style="margin-top:4px;padding-left:4px;">
  <div class="easyui-tabs" style="width:100%;height:360px;" >
    <div title="编辑交易单元列表" style="width:600px;height:100%;padding:10px;">
      <p style="color: red"><%=$ error %></p>
      <p style="color: green"><%=$ notice %></p>
      <h1>编辑交易单元列表</h1>
<table id="customers">
  <tr>
    <th>编号</th>
    <td><%= marketingunit.muid() %></td>
  </tr>

  <tr>
    <th>估值</th>
    <td><%= marketingunit.muvalue() %></td>
  </tr>

  <tr>
    <th>简称</th>
    <td><%= marketingunit.musname() %></td>
  </tr>

  <tr>
    <th>长名称</th>
    <td><%= marketingunit.muname() %></td>
  </tr>

  <tr>
    <th>日期</th>
    <td><%= marketingunit.mudate() %></td>
  </tr>

  <tr>
    <th>源单元编号</th>
    <td><%= marketingunit.srcUnitID() %></td>
  </tr>

  <tr>
    <th>目标单元编号</th>
    <td><%= marketingunit.destUnitID() %></td>
  </tr>

  <tr>
    <th>操作员编号</th>
    <td><%= marketingunit.operatorID() %></td>
  </tr>

</table>	
    </div>  
  </div>
        
<%== linkTo("Edit", urla("edit", marketingunit.muid())) %> |
<%== linkTo("Back", urla("index")) %>
        
</body>
</html>


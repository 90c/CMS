<!doctype html>
<%#include "marketingunit.h" %>
 <head>
	<meta charset="UTF-8">
	<link rel="stylesheet" type="text/css" href="/easyui/themes/default/easyui.css">
	<link rel="stylesheet" type="text/css" href="/easyui/themes/icon.css">
	<link rel="stylesheet" type="text/css" href="/common.css">
	<script type="text/javascript" src="/easyui/jquery.min.js"></script>
	<script type="text/javascript" src="/easyui/jquery.easyui.min.js"></script>
	<style type="text/css">

#customers
  {
  width:510px;
  border-collapse:collapse;
  empty-cells: hide;
  white-space: nowrap;
  }

#customers td, #customers th 
  {
  font-size:0.7em;
  border:1px solid #95B8E7;
  padding:3px 7px 2px 7px;
  }

#customers th 
  {
  font-size:0.7em;
  text-align:center;
  padding-top:5px;
  padding-bottom:4px;
  background-color:#F3F3F3;
  color:black;
  }

#customers tr, #customers td
  {
  color:#000000;
  }
</style>
</head>
 <body style="margin-top:4px;padding-left:4px;">
  <div class="easyui-tabs" style="width:100%;height:360px;" >
    <div title="交易单元管理" style="width:600px;height:100%;padding:10px;">
<table id="customers">
<tr>
<th>编号</th>
<th>估值</th>
<th>简称</th>
<th>长名称</th>
<th>日期</th>
<th>源单元编号</th>
<th>目标单元编号</th>
<th>操作员编号</th>
</tr>
<% tfetch(QList<Marketingunit>, list); %>
<% tfetch(int, c); %>
<% for (const auto &i : list) { %>
  <tr>
    <td><%= i.muid() %></td>
    <td><%= i.muvalue() %></td>
    <td><%= i.musname() %></td>
    <td><%= i.muname() %></td>
    <td><%= i.mudate() %></td>
    <td><%= i.srcUnitID() %></td>
    <td><%= i.destUnitID() %></td>
    <td><%= i.operatorID() %></td>
  </tr>
<% } %>
    <tr>
    <td>总值</td>
    <td><%= c %></td>
    </tr>
</table>	
  </div>  
  </div>
<div style="margin-top:4px">
    <ul type="none">
      <li style="float:left;"><a href="/marketingunit/reportform/" class="easyui-linkbutton" data-options="iconCls:'icon-back'" style="width:80px;">返回</a></li>
           
      </li>
    </ul>
  </div>
</body>
</html>


<!DOCTYPE html>
<%#include "marketingunit.h" %>
<%#include "assetsunit.h" %>
<% tfetch(Marketingunit, marketingunit); %>

<html>
<head>
  <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
  <title><%= controller()->name() + ": " + controller()->activeAction() %></title>
</head>
<body>
<p style="color: red"><%=$ error %></p>
<p style="color: green"><%=$ notice %></p>

<h1>查看</h1>
<dt>交易单元编号</dt><dd><%= marketingunit.muid() %></dd><br />
<dt>估值</dt><dd><%= marketingunit.muvalue() %></dd><br />
<dt>简称</dt><dd><%= marketingunit.musname() %></dd><br />
<dt>长名称</dt><dd><%= marketingunit.muname() %></dd><br />
<dt>日期</dt><dd><%= marketingunit.mudate() %></dd><br />
<% tfetch(AssetsUnit, asunit); %>
<dt>来源资产单元编号</dt><dd><%= asunit.assetsUnitID() %></dd><br />
<dt>来源管理人编号</dt><dd><%= asunit.managerID() %></dd><br />
<dt>来源资产单元简称</dt><dd><%= asunit.assetsUnitShortname() %></dd><br />
<% tfetch(AssetsUnit, assunit); %>
<dt>目标资产单元编号</dt><dd><%= assunit.assetsUnitID() %></dd><br />
<dt>目标管理人编号</dt><dd><%= assunit.managerID() %></dd><br />
<dt>目标资产单元简称</dt><dd><%= assunit.assetsUnitShortname() %></dd><br />

<%== linkTo("返回", urla("reportform")) %>

</body>
</html>
